%h3= t('.title')
= simple_form_for @thesis_examination, :as => :thesis_examination, :url => thesis_examination_path, :html => { :multipart => true, :id => 'record'} do |f|
  = render :partial => 'form', :locals => { :f => f }
  %div{ :class => 'span-24 buttons_left last' }
    = f.submit  t(:update), :class => "ui-state-default ui-corner-all button"
    = link_as_button t(:cancel), thesis_examination_path
