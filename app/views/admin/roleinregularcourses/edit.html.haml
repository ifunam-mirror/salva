%div{ :class => 'span-14 section_left last' }
  %h3= t('roleinregularcourses.edit.title') 
  = simple_form_for(@roleinregularcourse, :roleinregularcourse, :url => admin_roleinregularcourse_path) do |f|
    = render :partial => 'form', :locals => { :f => f}
    %div{ :class => 'span-14 buttons_left last' }
      = f.submit  t(:update), :class => "ui-state-default ui-corner-all button"
      = link_as_button t(:cancel), admin_roleinregularcourses_path
